<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mm.cs.entities.TradeFirst" table="tb_tradefirst">
		<id name="TradeFirstId" type="java.lang.String">
			<column name="TRADEFIRST_ID"></column>
			<generator class="uuid" />
		</id>
		
		<property name="TradeFirstName" type="java.lang.String">
			<column name="TRADEFIRST_NAME"></column>
		</property>
	</class>
</hibernate-mapping>
